mood_dict = {
    "ğŸ˜Š": "happy",
    "ğŸ˜¢": "sad",
    "ğŸ˜ ": "angry",
    "ğŸ˜": "cool",
    "ğŸ˜‚": "laughing",
    "ğŸ˜±": "scared",
